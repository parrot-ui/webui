{
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "semi": false,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "proseWrap": "never",
    "importOrder": [
        "^(react|react-dom)$",
        "^([a-z]|@[a-z])",
        "",
        ".*"
    ],
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ]
}
